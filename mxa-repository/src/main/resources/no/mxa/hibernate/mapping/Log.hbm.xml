<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="no.mxa.dto.LogDTO" table="LOG">
		<id name="id" type="java.lang.Long">
			<column name="ID" precision="9" scale="0" />
			<generator class="native">
				<param name="sequence">
					LOG_SEQ
				</param>
			</generator>
		</id>
		<many-to-one name="message" class="no.mxa.dto.MessageDTO" column="MESSAGEID" />
		<property name="time" type="java.sql.Timestamp" not-null="true">
			<column name="TIME" />
		</property>
		<property name="logType" type="java.lang.String" not-null="true">
			<column name="logType" length="1024" />
		</property>
		<property name="logMessage" type="java.lang.String" not-null="true">
			<column name="LOGMSG" length="1024" />
		</property>
	</class>
</hibernate-mapping>